{
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.0",
    "@create-figma-plugin/utilities": "^4.0.0",
    "preact": ">=10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.0",
    "@create-figma-plugin/tsconfig": "^4.0.0",
    "@figma/plugin-typings": "1.109.0",
